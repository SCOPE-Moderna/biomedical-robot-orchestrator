{
  "name": "nodes",
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447",
  "node-red": {
    "nodes": {
      "lower-case": "nodes/lower-case/lower-case.js",
      "grpc-ping": "nodes/grpc-ping/grpc-ping.js",
      "start-flow": "nodes/start-flow/start-flow.js",
      "xpeel-status": "nodes/xpeel-status/xpeel-status.js",
      "xpeel-reset": "nodes/xpeel-reset/xpeel-reset.js",
      "xpeel-xpeel": "nodes/xpeel-xpeel/xpeel-xpeel.js",
      "xpeel-sealcheck": "nodes/xpeel-sealcheck/xpeel-sealcheck.js",
      "xpeel-taperemaining": "nodes/xpeel-taperemaining/xpeel-taperemaining.js"
    }
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^22.7.5",
    "@types/node-red": "^1.3.5",
    "@types/pluralize": "^0.0.33",
    "prettier": "^3.3.3",
    "protoc-gen-ts": "^0.8.7",
    "typescript": "^5.6.3",
    "vite": "^6.2.0"
  },
  "scripts": {
    "format": "prettier --write nodes --ignore-path .gitignore",
    "lint": "prettier --check nodes --ignore-path .gitignore",
    "compile": "vite build && tsc -p .",
    "protos": "protoc --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" -I../protos --ts_opt=esModuleInterop=true --ts_out=./node_connector_pb2 ../protos/node_connector.proto",
    "protos-win": "protoc --plugin=\"protoc-gen-ts.cmd=./node_modules/.bin/protoc-gen-ts\" -I../protos --ts_opt=esModuleInterop=true --ts_out=./node_connector_pb2 ../protos/node_connector.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.2",
    "google-protobuf": "^3.21.4",
    "pluralize": "^8.0.0"
  }
}
